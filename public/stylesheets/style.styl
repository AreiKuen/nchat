*
  margin 0px
  padding 0px
body
  font: 14px "Lucida Grande", Helvetica, Arial, sans-serif
  width 100%
  height 100%
p
  color: #D00
#container
  position fixed
  top 0
  left 0
  width 100%
  height 99.8%
#usersHolder
  text-align center
  float left
  width 13%
  height 100%
  box-shadow 0px 2px 5px #666
  overflow scroll
  overflow-x hidden
  z-index 9
#usersTitle
  background #C00
  color #FFF
  margin 0px 0px 2px 0px
  padding 5px
.user
  padding 4px 0px
  font-size: 1.1em
.user:hover
  background #D00 !important
  color #FFF
  font-weight bold
  cursor pointer
#conversationHolder
  position relative
  width 87%
  height 100%
  float left
#tabHolder
  height 25px
  width 100%
  box-shadow 3px 0px 5px #666
  z-index 7
#rooms
  float left
  background #C00
  font-weight bold
  padding 4px 0px 6px 0px
  width 25px
  height 15px
  color #FFF
  text-align center
  border-right 1px solid #000
  cursor pointer
#rooms:hover
  background #D00
.tab
  position relative
  display inline-block
  float left
  padding 3px 25px 3px 10px
  border-right 1px solid #000
  height 19px
  text-align center
  cursor pointer
.tab:hover
  background #D00 !important
  color #FFF !important
.x
  position absolute
  top 3px
  height 19px
  right 3px
  padding 0px 5px 0px 5px
  font-weight bold
.x:hover
  background #FFF
  color #D00
.conversation
  position absolute
  padding 2px 0px 0px 8px
  top 29px
  bottom 69px
  left 0
  right 316px
  overflow scroll
  overflow-x hidden
  font-size 1.2em
  z-index 5
.media
  text-align center
  position absolute
  top 29px
  width 316px
  bottom 69px
  right 0
  z-index 0
  overflow scroll
  overflow-x hidden
#chatControlsHolder
  position absolute
  bottom 0
  box-shadow 3px 1px 5px #666
  height 65px
  width 100%
  z-index 6
#chatInput
  position absolute
  left 0
  right 103px
  padding 0px 3px
  height 93%
  width 90%
  margin 2px 6px 3px 6px
  resize none
  font-family Verdana
  font-size 1.5em
  border none
#sendButton
  position absolute
  right 0
  margin 3px 3px 0px 0px
  height 60px
  width 100px
  border: none
  font-weight bold
  background #C00
  color #FFF
  font-size 1.4em
  cursor pointer
#sendButton:hover
  background #D00
#loading
  position fixed
  top 0
  left 0
  width 100%
  height 100%
  background #FFF
  background-image url('/images/background.png')
  z-index: 100
#loadHolder
  position relative
  text-align center
  top 15%
  width 706px
  height 300px
  margin 0px auto
#loadimg
  width 200px
  height 200px
#loadText
  margin-top 15px
  font-size 1.1em
  font-weight bold
#nameInput
  margin-top 45px
  box-shadow 0px 0px 3px #666
  height 40px
  width 200px
  padding 0px 10px
  border-radius 5px
  font-weight bold
  font-size 1.3em
  border none
#nameSubmit
  width 100px
  height 40px
  margin 20px 0px 0px 0px
  border none
  background #C00
  color #FFF
  font-size 1.5em
  font-weight bold
  cursor pointer
#roomList
  text-align left
  margin 0px auto
  list-style none
  width 100px
  font-size 1.2em
.roomListRoom
  padding 5px 0px
  background #FFF
  cursor pointer
.roomListRoom:hover
  background #D00 !important
  color #FFF
#joinRoom
  background #C00
  color #FFF
  font-weight bold
  font-size 1.5em
  border none
  padding 5px 10px
  margin-top 10px
  cursor pointer
#joinRoom:hover
  background #D00
.smiley
  width 18px
  height 18px
  margin-bottom -3px
#copyrightHolder
  position absolute
  bottom 0
  height 20px
  width 100%
  text-align center
#copyright
  font-weight bold
  text-shadow 0px 0px 1px #B88
  font-size 0.9em